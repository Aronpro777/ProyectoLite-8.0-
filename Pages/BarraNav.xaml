<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProyectoLite__8._0_.Pages.BarraNav">


    <Grid Padding="10" ColumnSpacing="30"
          HeightRequest="70"
          BackgroundColor="#A94438">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>

        <Image Source="icon_home.png" Grid.Column="0" HeightRequest="24" HorizontalOptions="Center"/>
        <Image Source="icon_search.png" Grid.Column="1" HeightRequest="24" HorizontalOptions="Center"/>
        <Image Source="icon_user.png" Grid.Column="4" HeightRequest="24" HorizontalOptions="Center"/>
    </Grid>
</ContentView>
